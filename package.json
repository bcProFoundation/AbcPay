  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "description": "An Ionic project",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ionic build --prod --release",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "config": "ts-node ./scripts/setenv.ts",
    "clean": "rm -rf android && rm -rf ios",
    "start:android": "ionic build && ionic cap sync android",
    "start:ios": "ionic build && ionic cap sync ios && ionic cap open ios",
    "build:android": "npm run env:dev && ionic build && ionic cap sync android && cd android && ./gradlew assembleDebug && cd ..",
    "build:ios": "npm run env:dev && ionic build && ionic cap sync ios",
    "build:android-release": "npm run env:prod && npm run build-prod && ionic cap sync android && cd android && ./gradlew assembleRelease && cd ..",
    "build:ios-release": "npm run env:prod && npm run build-prod && ionic cap sync ios",
    "prepare:abcpay": "npm run clean && ionic cap add ios && ionic cap add android && npm run fcm:android && npm run fcm:ios && npm run resources",
    "prepare:android": "npm run env:dev && npm run build && ionic cap sync android && npm run resources",
    "fcm:android": "mkdir -p android/app; echo android/app | xargs -n 1 cp google-services.json",
    "fcm:ios": "mkdir -p ios/App/app; echo ios/App/app | xargs -n 1 cp GoogleService-Info.plist",
    "ngsw-config": "npx ngsw-config www /ngsw.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js www/",
    "env:desktop": "npm run config -- --environment=desktop \n\n# Environment set to: desktop\n\n",
    "env:prod": "npm run config -- --environment=production \n\n# Environment set to: prod\n\n",
    "env:dev": "npm run config -- --environment=development \n\n# Environment set to: dev\n\n ",
    "apply": "npm run apply:android && npm run apply:ios",
    "apply:android": "npx tsc configure/configure-android.ts && node configure/configure-android.js",
    "apply:ios": "npx tsc configure/configure-ios.ts && node configure/configure-ios.js",
    "env:custom": "npm run config -- --environment=development --awsUrl=https://aws.abcpay.cash/bws/api \n\n# Environment set to: dev\n\n",
    "resources": "capacitor-resources -p android,ios",
    "generate:android-bundle": "cd android && ./gradlew task && ./gradlew bundle",
    "sign:android": "bundletool build-apks --mode=universal --bundle=./android/app/build/outputs/bundle/release/app-release.aab --output=./apks/apcpay-release.apks --ks=release.jks --ks-pass=pass:123456 --ks-key-alias=release --key-pass=pass:123456",
    "final:android-release": "npm run build:android-release && rm -rf apks && npm run generate:android-bundle && npm run sign:android",
    "final:android-apk": "cd android && ./gradlew assembleRelease && cd ..",
    "final:android-debug": "npm run build:android && rm -rf apks && bundletool build-apks --bundle=./android/app/build/outputs/bundle/release/app-release.aab --output=./apks/apcpay-debug.apks",
    "final:mobile": "ionic build --prod && npx cap copy && npx cap sync"
  },
